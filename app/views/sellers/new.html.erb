<%= form_for Seller.new do |f| %>
	<%= f.label :seller_username %>
	<%= f.text_field :seller_username, id:"seller_username" %>

	<%= f.label :email %>
	<%= f.text_field :email, id:"email" %>

	<%= f.label :password %>
	<%= f.text_field :password, id:"password" %>

	<%= f.label :password_confirmation %>
	<%= f.text_field :password_confirmation, id:"password_confirmation" %>

	<%= f.submit :save %>

<% end %>